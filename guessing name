import random 

random_num = random.randint (1,100)

while True:

    gusse = int(input("enter gusse: "))
   
    if random_num > gusse:
        print ("gusse higher")
        
    elif random_num < gusse:
        print ("gusse lower")
        
    elif random_num == gusse:
        print ("Correctr")

    choice = input("do you want to try agian Y/N?: ")
    
    if choice == ("Y"):
        restart 
    
    elif choice == ("N"):
        print ("thank you for playing")
 
